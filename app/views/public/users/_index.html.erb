<table class="table">
  <thead>
    <tr>
      <th>ニックネーム</th>
      <th>性別</th>
      <th>年齢</th>
      <th>自己紹介</th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
    <tr>
      <% if user.is_deleted != true %>
      <td>
        <%= image_tag user.get_profile_image, style: "border-radius: 50%; object-fit:cover;", size: '100x100' %><br>
        <%= link_to user.name, user_path(user.id) %>
      </td>
      <td><%= user.gender_i18n %></td>
      <td><%= user.age %>歳</td>
      <td><%= user.introduction %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
  </table>